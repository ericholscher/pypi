<tal:if-user condition="app/username">

<tr><td bgcolor="#99ccff">Welcome 
<span tal:replace="app/username"/></td></tr>
<tr><td bgcolor="#99ccff">
 <a tal:attributes="href python: app.link_action('user_form')">
Your details</a></td></tr>
<tr><td bgcolor="#99ccff">
 <a tal:attributes="href python: app.link_action('logout')">
Logout</a></td></tr>

<tal:let define="packages python: app.store.user_packages(app.username)">
<tal:if-packages condition="packages">
<tr><td bgcolor="#003366"><b><font color="#ffffff">
Your Packages
</font></b></td></tr>
<tr tal:repeat="pkg_info packages">
 <td bgcolor="#99ccff">
  <a tal:attributes="href python: app.link_action('pkg_edit', name=pkg_info[0])"
   tal:content="python: pkg_info[0]">package</a>
 </td>
</tr>
</tal:if-packages>
</tal:let>
</tal:if-user>

<tal:if-not-user condition="not: app/username">
<tr><td bgcolor="#003366"><b><font color="#ffffff">
Not Logged In
</font></b></td></tr>
<tr><td bgcolor="#99ccff">
 <a tal:attributes="href python: app.link_action('register_form')">Register</a></td></tr>
<tr><td bgcolor="#99ccff">
 <a tal:attributes="href python: app.link_action('login')">Login</a></td></tr>
<tr><td bgcolor="#99ccff">
 <a tal:attributes="href python: app.link_action('forgotten_password_form')">
Lost Login?</a></td></tr>
<tr><td bgcolor="#99ccff"></td></tr>
</tal:if-not-user>

